<div id="saimaniq-questions-bar" class="container mb-4 mx-0">
    <div class="{{{ classes }}}">{{{ question_answered }}}</div>
    {{#questionbar}}
    <div class="progress rounded-0">
        <div class="progress-bar" role="progressbar" aria-valuenow="{{{ attempted }}}" aria-valuemin="0" aria-valuemax="{{{ total }}}" style="width:{{{ percentage }}}%">
            <span class="sr-only">{{{ percentage }}}% completed</span>
        </div>
    </div>
    {{/questionbar}}
</div>