<div id="exam-submit-confirmation" class="exam-modal exam-modal-show {{^isreview }}d-none{{/isreview }}">
    <div class="exam-modal-parent">
        <div class="exam-modal-head">
            <div class="exam-modal-header text-center">{{{ quiz_name }}}</div>
            <div class="exam-modal-hr">&nbsp;</div>
        </div>
    </div>
    <div class="exam-modal-body">
        <div class="exam-modal-icon">
            <span class="fa-stack">
                <i class="fa fa-stack-2x fa-circle-o "></i>
                <i class="fa fa-stack-1x {{^hybrid}}fa-check{{/hybrid}}{{#hybrid}}fa-arrow-right{{/hybrid}}"></i>
            </span>
        </div>
        <div class="exam-modal-text">
            {{^hybrid}}{{# str }} modaltext, theme_saimaniq {{/ str }}{{/hybrid}}
            {{#hybrid}}{{# str }} qruploadmodaltext, theme_saimaniq {{/ str }}{{/hybrid}}  
        </div>
    </div>
</div>